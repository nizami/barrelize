  "$schema": "schema.json",
  "barrels": [
    {
      "root": "src",
      "name": "index.ts",
      "include": ["**/*.ts"],
      "exclude": ["**/*.test.ts", "schema/**"],
      "order": ["cli", "log"],
      "bracketSpacing": false,
      "replace": {
